<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Frag_main_maps">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#cccfff">

        <Button
            android:id="@+id/btnListMode2"
            android:layout_width="117dp"
            android:layout_height="46dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/btn_style"
            android:text="List Mode"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.243"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvChooseMode2" />

        <Button
            android:id="@+id/btnMapMode2"
            android:layout_width="121dp"
            android:layout_height="46dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/btn_style_clicked"
            android:text="Map Mode"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.38"
            app:layout_constraintStart_toEndOf="@+id/btnListMode2"
            app:layout_constraintTop_toBottomOf="@+id/tvChooseMode2" />

        <Button
            android:id="@+id/btnNewNote2"
            android:layout_width="160dp"
            android:layout_height="42dp"
            android:layout_marginTop="76dp"
            android:background="@drawable/btn_style"
            android:text="Create New Note"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvChooseMode2" />

        <Button
            android:id="@+id/btnLogout2"
            android:layout_width="146dp"
            android:layout_height="38dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/btn_style"
            android:text="Logout System"
            android:textStyle="bold"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnNewNote2" />

        <TextView
            android:id="@+id/welcomeUser2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/smoedy"
            android:text="WELCOME"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvNotes2"
            android:layout_width="257dp"
            android:layout_height="40dp"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/fortune"
            android:gravity="center"
            android:text="here are your location notes:"
            android:textColor="#000000"
            android:textSize="34sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/welcomeUser2" />

        <TextView
            android:id="@+id/tvChooseMode2"
            android:layout_width="231dp"
            android:layout_height="31dp"
            android:layout_marginTop="28dp"
            android:gravity="center"
            android:text="choose mode:"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mapView" />

        <com.google.android.gms.maps.MapView
            android:id="@+id/mapView"
            android:layout_width="371dp"
            android:layout_height="320dp"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvNotes2"
            android:apiKey="AIzaSyBXC7uHw4Fo38ToudW5FpLrnDmka0Y8G2k"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:visibility="visible"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>